{
  "name": "fish-lsp",
  "version": "1.0.01",
  "description": "LSP implementation for fish/fish-shell",
  "author": "ndonfris",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndonfris/fish-lsp.git"
  },
  "keywords": [
    "lsp",
    "fish",
    "fish-shell",
    "language-server-protocol",
    "language-server"
  ],
  "bin": {
    "fish-lsp": "./bin/fish-lsp"
  },
  "main": "./out/out.js",
  "typings": "./out/server.d.ts",
  "scripts": {
    "pre:clean": "yarn exec rimraf lib *.tsbuildinfo out",
    "clean": "yarn run pre:clean && tsc -b && echo '' > logs.txt",
    "clean-nodemodules": "yarn rimraf out *.tsbuildinfo node_modules && pnpm install",
    "clean:all": "yarn exec rimraf out *.tsbuildinfo node_modules tree-sitter-fish.wasm",
    "fresh": "yarn run clean:all && pnpm install && yarn run setup",
    "test": "yarn run jest --runInBand --watch --verbose true",
    "compile": "yarn tsc -b",
    "watch": "yarn tsc -b -w",
    "build:wasm": "tree-sitter build node_modules/tree-sitter-fish/ -o tree-sitter-fish.wasm",
    "build-wasm": "./scripts/build-fish-wasm.sh",
    "build-binaries": "npx pkg --targets node18-linux,node18-macos,node18-win ./out/cli.js -o build/fish-lsp",
    "setup": "./scripts/setup.sh"
  },
  "eslintIgnore": [
    "!.eslintrc.cjs"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn run clean",
      "post-merge": "yarn"
    }
  },
  "dependencies": {
    "@babel/generator": "7.24.1",
    "@babel/parser": "7.24.1",
    "@esdmr/tree-sitter-fish": "^3.5.1",
    "@jridgewell/trace-mapping": "0.3.25",
    "ansi-escapes": "6.2.1",
    "ansi-styles": "6.2.1",
    "brace-expansion": "4.0.0",
    "camelcase": "8.0.0",
    "ci-info": "4.0.0",
    "color-name": "2.0.0",
    "colors": "^1.4.0",
    "commander": "^12.0.0",
    "deepmerge": "^4.3.1",
    "diff": "5.2.0",
    "escape-string-regexp": "5.0.0",
    "eslint-scope": "8.0.1",
    "estraverse": "5.3.0",
    "find-up": "7.0.0",
    "fs-extra": "11.2.0",
    "glob": "10.3.12",
    "globals": "15.0.0",
    "globby": "14.0.1",
    "has-flag": "5.0.1",
    "lru-cache": "10.2.0",
    "lua-json": "^1.0.1",
    "marked": "12.0.1",
    "minimatch": "9.0.4",
    "p-limit": "5.0.0",
    "pkg-dir": "8.0.0",
    "readable-stream": "4.5.2",
    "slash": "5.1.0",
    "strip-bom": "5.0.0",
    "strip-json-comments": "5.0.1",
    "supports-color": "9.4.0",
    "ts-jest": "^29.1.2",
    "ts-node": "10.9.2",
    "tsc": "^2.0.4",
    "type-fest": "4.14.0",
    "typescript": "^5.4.3",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-protocol": "3.17.5",
    "vscode-languageserver-textdocument": "1.0.11",
    "vscode-uri": "^3.0.8",
    "web-tree-sitter": "^0.22.2",
    "yn": "5.0.0"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^20.1.3",
    "@types/chai": "^4.3.14",
    "@types/fs-extra": "^11.0.4",
    "@types/lua-json": "^1.0.3",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "chai": "^5.1.0",
    "eslint": "^8.57.0",
    "fast-glob": "^3.3.2",
    "jest": "^29.7.0",
    "pkg": "^5.8.1",
    "rimraf": "^5.0.5",
    "tree-sitter-cli": "^0.22.2",
    "tree-sitter-fish": "github:ram02z/tree-sitter-fish"
  },
  "enabledApiProposals": [
    "inlineCompletions"
  ],
  "jest": {
    "preset": "ts-jest",
    "verbose": true,
    "clearMocks": true,
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/out"
    ],
    "bail": 1,
    "moduleNameMapper": {
      "^completion/(.*)$": "<rootDir>/src/utils/completion/$1",
      "^utils/(.*)$": "<rootDir>/src/utils/$1",
      "^test-data/(.*)$": "<rootDir>/test-data/$1"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.ts?$": "ts-jest",
      "^.+\\.js?$": "babel-jest"
    },
    "testRegex": "\\.test\\.ts$",
    "testTimeout": 8000,
    "maxWorkers": "50%/4"
  },
  "resolutions": {
    "chalk": "5.3.0",
    "fs-extra": "10.1.0",
    "glob": "8.1.0",
    "globby": "13.2.2",
    "marked": "4.3.0",
    "semver": "7.6.0",
    "vscode-languageserver-protocol": "3.17.5",
    "source-map-support": "0.5.21",
    "ts-node": "10.9.2",
    "minimatch": "5.0.1",
    "brace-expansion": "2.0.1",
    "slash": "3.0.0",
    "ansi-escapes": "4.3.2",
    "type-fest": "3.13.1",
    "has-flag": "4.0.0",
    "supports-color": "7.2.0",
    "find-up": "6.3.0",
    "locate-path": "7.2.0",
    "path-exists": "5.0.0",
    "p-locate": "6.0.0",
    "p-limit": "4.0.0",
    "yocto-queue": "1.0.0",
    "lru-cache": "6.0.0",
    "yallist": "4.0.0",
    "prebuild-install": "7.1.2",
    "strip-json-comments": "3.1.1",
    "readable-stream": "3.6.2",
    "string_decoder": "1.3.0",
    "safe-buffer": "5.2.1",
    "json5": "2.2.3",
    "yargs-parser": "21.1.1",
    "ci-info": "3.9.0",
    "ansi-styles": "4.3.0",
    "color-convert": "2.0.1",
    "color-name": "1.1.4",
    "vscode-jsonrpc": "8.2.0",
    "vscode-languageserver-types": "3.17.5",
    "escape-string-regexp": "4.0.0",
    "eslint-scope": "7.2.2",
    "estraverse": "4.3.0",
    "ms": "2.1.3",
    "tslib": "2.6.2",
    "yargs": "17.7.2",
    "cliui": "8.0.1",
    "glob-parent": "6.0.2",
    "globals": "13.24.0",
    "js-yaml": "4.1.0",
    "resolve-from": "5.0.0",
    "argparse": "2.0.1",
    "pkg-dir": "5.0.0",
    "@jridgewell/trace-mapping": "0.3.9",
    "istanbul-lib-instrument": "6.0.2",
    "@babel/parser": "7.24.0",
    "@babel/generator": "7.23.6",
    "@babel/types": "7.24.0",
    "@babel/helper-string-parser": "7.24.1",
    "camelcase": "6.3.0",
    "is-core-module": "2.13.1",
    "strip-bom": "4.0.0",
    "diff": "5.0.0",
    "yn": "3.1.1"
  },
  "peerDependencies": {
    "typescript": "^5.4.3"
  }
}
