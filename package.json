{
  "name": "typescript-language-server",
  "version": "1.1.2",
  "description": "Language Server Protocol (LSP) implementation for TypeScript using tsserver",
  "author": "TypeFox and others",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/typescript-language-server/typescript-language-server.git"
  },
  "engines": {
    "node": ">=14.16"
  },
  "files": [
    "lib"
  ],
  "bin": {
    "fish-language-server": "./lib/src/index.js"
  },
  "scripts": {
    "clean": "rimraf lib *.tsbuildinfo",
    "prepare:tests": "rm -f test_data/fish_files/*.fish && yarn run prepare:install:tests",
    "prepare:install:tests": "cd test_data && ./install_scripts/generate_largest_fish_files.fish && cd ..",
    "test": "jest --runInBand --watch --verbose='false'",
    "compile": "tsc -b",
    "watch": "tsc -b --watch --verbose"
  },
  "eslintIgnore": [
    "!.eslintrc.cjs"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn run clean",
      "post-merge": "yarn"
    }
  },
  "dependencies": {
    "commander": "^9.4.0",
    "deepmerge": "^4.2.2",
    "fs-extra": "^10.1.0",
    "git": "^0.1.5",
    "global": "^4.4.0",
    "p-debounce": "^4.0.0",
    "pkg-up": "^4.0.0",
    "semver": "^7.3.7",
    "tempy": "^3.0.0",
    "top-level-await": "^1.1.0",
    "tree-sitter-fish": "github:ram02z/tree-sitter-fish",
    "ts-jest": "^29.0.1",
    "unixify": "^1.0.0",
    "url": "^0.11.0",
    "vscode-languageserver": "^8.0.2",
    "vscode-languageserver-protocol": "^3.17.2",
    "vscode-languageserver-textdocument": "1.0.7",
    "vscode-uri": "^3.0.6",
    "web-tree-sitter": "^0.20.7",
    "which": "^2.0.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.0.3",
    "@types/node": "^16.11.56",
    "@types/semver": "^7.3.12",
    "@types/which": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "chai": "^4.3.6",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.0",
    "husky": "^4.3.8",
    "jest": "^29.0.3",
    "mocha": "^10.0.0",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}
